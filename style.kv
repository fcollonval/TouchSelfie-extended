#:kivy 1.10.1

<Button>:
    background_normal: 'atlas://data/defaulttheme/button'
    background_down: 'atlas://data/defaulttheme/button_pressed'
    background_disabled_normal: 'atlas://data/defaulttheme/button_disabled'
    background_disabled_down: 'atlas://data/defaulttheme/button_disabled_pressed'

<VKeyboard>:
    key_background_normal: 'atlas://data/defaulttheme/vkeyboard_key_normal'
    # key_background_down: 'atlas://data/defaulttheme/vkeyboard_key_down'
    # key_background_disabled_normal: 'atlas://data/defaulttheme/vkeyboard_disabled_key_normal'
    # key_border: [128, 128, 128, 255]
    # background can NOT be empty
    background: "atlas://data/defaulttheme/vkeyboard_background"
    # background_border: [1, 1, 1, 1]
    background_color: [1, 1, 1, 1]
    background_disable: "atlas://data/defaulttheme/vkeyboard_disabled_background"

